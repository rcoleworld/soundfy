{
    "compilerOptions": {
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": true,
      "outDir": "./dist/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5",
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "node",
      "declaration": true,
    },
    "include": [
        "./src/**/**/*"
    ]
  }
